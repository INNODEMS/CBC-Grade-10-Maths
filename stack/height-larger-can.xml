    <questionvariables original-id="height-larger-can-1-1-9" assembly-id="height-larger-can-1-1-9" unique-id="height-larger-can-1-1-9">
      <text original-id="height-larger-can-1-1-9-1" assembly-id="height-larger-can-1-1-9-1" unique-id="height-larger-can-1-1-9-1">/* Generate perfect-volume values for a and b */
volume_list_a:rand ([8,27,64,125,216,343,512]);  /* extend if desired */
volume_list_b:rand ([729,1000,1331,1728,2197]);  /* extend if desired */

/* height of the smaller can */
c: rand(16) + 7;
/*cube root of b/a*/
cuberoot:(volume_list_b/volume_list_a)^(1/3);
/* height of the cone*/
height:float(cuberoot*c);
ta:decimalplaces(height,2);

/*mistakes*/
/*Using the volume ratio directly as the height ratio*/
ta1:decimalplaces((volume_list_b/volume_list_a)*c,2);</text>
