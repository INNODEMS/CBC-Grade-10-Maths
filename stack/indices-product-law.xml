    <questionvariables original-id="indices-product-law-1-1-9" assembly-id="indices-product-law-1-1-9" unique-id="indices-product-law-1-1-9">
      <text original-id="indices-product-law-1-1-9-1" assembly-id="indices-product-law-1-1-9-1" unique-id="indices-product-law-1-1-9-1">/* Random base and exponents */
base: rand([2,3,5,7]);
exp1: rand([1,2,3,4]);
exp2: rand([1,2,3,4]);

/* Correct simplified exponent */
exp_total: exp1 + exp2;

/* Full expression before simplification */
expr: base^exp1 * base^exp2;

/* Common student mistakes */
m1: base^(exp1*exp2);     /* multiplies exponents instead of adding */
m2: (base^exp1) + (base^exp2);  /* adds powers instead of multiplying */
m3: base^(exp1 - exp2);   /* subtracts instead of adding */
</text>
    </questionvariables>
