    <questionvariables original-id="prob-exp-sample-space-1-1-9" assembly-id="prob-exp-sample-space-1-1-9" unique-id="prob-exp-sample-space-1-1-9">
      <text original-id="prob-exp-sample-space-1-1-9-1" assembly-id="prob-exp-sample-space-1-1-9-1" unique-id="prob-exp-sample-space-1-1-9-1">/* Randomly choose the experiment */
exp_choice: rand([1,2,3]);

/* Define the experiment description as a string */
experiment_text: if exp_choice=1 then "Tossing a fair coin once."
                  elseif exp_choice=2 then "Rolling a fair six-sided die once."
                  else "Selecting a day at random from the weekend.";

/* Sample spaces */
S_text: if exp_choice=1 then "{H, T}"
         elseif exp_choice=2 then "{1, 2, 3, 4, 5, 6}"
         else "{Sat, Sun}";
</text>
    </questionvariables>
