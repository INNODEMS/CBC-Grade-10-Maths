    <questionvariables original-id="negative-index-laws-1-1-9" assembly-id="negative-index-laws-1-1-9" unique-id="negative-index-laws-1-1-9">
      <text original-id="negative-index-laws-1-1-9-1" assembly-id="negative-index-laws-1-1-9-1" unique-id="negative-index-laws-1-1-9-1">/* Choose a variable */
var: rand([x,y,a,b,p,q,r]);

/* Random positive coefficient */
coef: rand_with_step(2,9,1);

/* Random power for negative index */
n: rand([1,2,3]);

/* Expression: coef * var^(-n) */
expr: coef * var^(-n);

/* Correct answer: coef/(var^n) */
ans: coef/(var^n);

/* Wrong answers */
m1: coef * var^n;       /* flips rule */
m2: -coef/(var^n);      /* introduces sign error */
m3: coef * -n;          /* confuses exponent with multiplication */
</text>
    </questionvariables>
