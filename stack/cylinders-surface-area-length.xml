    <name original-id="cylinders-surface-area-length-1-1-1" assembly-id="cylinders-surface-area-length-1-1-1" unique-id="cylinders-surface-area-length-1-1-1">
      <text original-id="cylinders-surface-area-length-1-1-1-1" assembly-id="cylinders-surface-area-length-1-1-1-1" unique-id="cylinders-surface-area-length-1-1-1-1">Similarity and Enlargement Cylinders _Surface Area, Length, and Volume Relationships (copy)</text>
    </name>
    <questiontext format="moodle_auto_format" original-id="cylinders-surface-area-length-1-1-2" assembly-id="cylinders-surface-area-length-1-1-2" unique-id="cylinders-surface-area-length-1-1-2">
      <text original-id="cylinders-surface-area-length-1-1-2-1" assembly-id="cylinders-surface-area-length-1-1-2-1" unique-id="cylinders-surface-area-length-1-1-2-1">&lt;p&gt;&lt;strong&gt;Two cylinders, P and Q, are mathematically similar.&lt;/strong&gt;&lt;/p&gt;

[[jsxgraph]]
var board = JXG.JSXGraph.initBoard('jxgbox', {
    boundingbox: [-60, -60, 90, 90],
    axis: false,
    visible: false,
    showCopyright: false,
    showNavigation: true,
    keepAspectRatio: true
});

/* Cylinder P */
var radiusP = 10;
var heightP = 60;

// Bottom ellipse (P)
board.create('ellipse', [
    [-25, 0],
    [-25 + radiusP, 0],
    [-25, 3]
], {strokeColor: '#000', fillColor: '#ccc', fillOpacity: 0.3, fixed: true});

// Top ellipse (P)
board.create('ellipse', [
    [-25, heightP],
    [-25 + radiusP, heightP],
    [-25, heightP + 3]
], {strokeColor: '#000', fillColor: '#ccc', fillOpacity: 0.3, fixed: true});

// Side lines (P)
board.create('segment', [[-26.7, 0], [-26.7, heightP]], {strokeColor: '#000', fixed: true});
board.create('segment', [[-13.5, 0], [-13.5, heightP]], {strokeColor: '#000', fixed: true});

// Labels (P)
board.create('text', [-25 + radiusP + 3, heightP / 2, 'Length = {#L_P#} cm'], {fixed: true});
board.create('text', [-20, heightP + 5, '&lt;b&gt;Cylinder P&lt;/b&gt;'], {fixed: true});

/* Cylinder Q */
var radiusQ = 15;
var heightQ = 80;

// Bottom ellipse (Q)
board.create('ellipse', [
    [20, 0],
    [20 + radiusQ, 0],
    [20, 3]
], {strokeColor: '#000', fillColor: '#cde', fillOpacity: 0.3, fixed: true});

// Top ellipse (Q)
board.create('ellipse', [
    [20, heightQ],
    [20 + radiusQ, heightQ],
    [20, heightQ + 3]
], {strokeColor: '#000', fillColor: '#cde', fillOpacity: 0.3, fixed: true});

// Side lines (Q)
board.create('segment', [[18.3, 0], [18.3, heightQ]], {strokeColor: '#000', fixed: true});
board.create('segment', [[35, 0], [35, heightQ]], {strokeColor: '#000', fixed: true});

// Labels (Q)
board.create('text', [20 + radiusQ + 3, heightQ / 2, 'Length = ?'], {fixed: true});
board.create('text', [25, heightQ + 5, '&lt;b&gt;Cylinder Q&lt;/b&gt;'], {fixed: true});
[[/jsxgraph]]

&lt;hr&gt;
&lt;ul&gt;
  &lt;li&gt;Total surface area of cylinder &lt;strong&gt;P&lt;/strong&gt; = {@ SA_P @} cm²&lt;/li&gt;
  &lt;li&gt;Total surface area of cylinder &lt;strong&gt;Q&lt;/strong&gt; = {@ SA_Q @} cm²&lt;/li&gt;
  &lt;li&gt;Length of cylinder &lt;strong&gt;P&lt;/strong&gt; = {@ L_P @} cm&lt;/li&gt;
&lt;/ul&gt;

&lt;p&gt;&lt;strong&gt;(a)&lt;/strong&gt; Work out the length of cylinder Q.&lt;/p&gt;
&lt;p&gt;Give your answer in cm.&lt;/p&gt;

&lt;p&gt;Length of cylinder Q = [[input:ans1]] [[validation:ans1]] [[feedback:prt1]]&lt;/p&gt;

&lt;hr&gt;

&lt;p&gt;The volume of cylinder &lt;strong&gt;P&lt;/strong&gt; is {@ V_P @} cm³.&lt;/p&gt;
&lt;p&gt;&lt;strong&gt;(b)&lt;/strong&gt; Work out the volume of cylinder Q.&lt;/p&gt;
&lt;p&gt;Give your answer in cm³.&lt;/p&gt;

&lt;p&gt;Volume of cylinder Q = [[input:ans2]] [[validation:ans2]] [[feedback:prt2]]&lt;/p&gt;

&lt;hr&gt;

&lt;details&gt;
  &lt;summary&gt;&lt;strong&gt;Hints (click to expand)&lt;/strong&gt;&lt;/summary&gt;
  &lt;ul&gt;
    &lt;li&gt;For similar solids: area scale factor = (length scale factor)²&lt;/li&gt;
    &lt;li&gt;Volume scale factor = (length scale factor)³&lt;/li&gt;
    &lt;li&gt;Length scale factor = √(area scale factor)&lt;/li&gt;
  &lt;/ul&gt;
&lt;/details&gt;</text>
    </questiontext>
    <generalfeedback format="moodle_auto_format" original-id="cylinders-surface-area-length-1-1-3" assembly-id="cylinders-surface-area-length-1-1-3" unique-id="cylinders-surface-area-length-1-1-3">
      <text original-id="cylinders-surface-area-length-1-1-3-1" assembly-id="cylinders-surface-area-length-1-1-3-1" unique-id="cylinders-surface-area-length-1-1-3-1">&lt;h5&gt;&lt;strong&gt;Worked Solution&lt;/strong&gt;&lt;/h5&gt;

&lt;p&gt;&lt;strong&gt;Lets first look at what we are given:&lt;/strong&gt;&lt;/p&gt;
&lt;ul&gt;
  &lt;li&gt;Total surface area of cylinder &lt;strong&gt;P&lt;/strong&gt; = {@ SA_P @} cm²&lt;/li&gt;
  &lt;li&gt;Total surface area of cylinder &lt;strong&gt;Q&lt;/strong&gt; = {@ SA_Q @} cm²&lt;/li&gt;
  &lt;li&gt;Length of cylinder &lt;strong&gt;P&lt;/strong&gt; = {@ L_P @} cm&lt;/li&gt;
  &lt;li&gt;Volume of cylinder &lt;strong&gt;P&lt;/strong&gt; = {@ V_P @} cm³&lt;/li&gt;
&lt;/ul&gt;

&lt;h5&gt;&lt;strong&gt;(a)&lt;/strong&gt; Find the length of cylinder Q&lt;/h5&gt;

&lt;ol&gt;
  &lt;li&gt;Find the area (surface area) scale factor:&lt;br&gt;
  \( \text{Area scale factor} = \dfrac{S_Q}{S_P} = \dfrac{{@ SA_Q @}}{{@ SA_P @}} = {@ scale_factor_area @} \)&lt;/li&gt;&lt;br&gt;

  &lt;li&gt;Find the linear (length) scale factor:&lt;br&gt;
  Since area scale factor = (length scale factor)²,&lt;br&gt;
  \( \text{Length scale factor} = \sqrt{{@ scale_factor_area @}} = {@ scale_factor_length @} \)&lt;/li&gt;&lt;br&gt;

  &lt;li&gt;Find the length of Q:&lt;br&gt;
  \( L_Q = {@ L_P @} \times {@ scale_factor_length @} = {@ L_Q @}\ \text{cm} \)&lt;/li&gt;
&lt;/ol&gt;

&lt;p&gt;&lt;strong&gt;Answer (a):&lt;/strong&gt; {@ ta_ans1 @} cm&lt;/p&gt;

&lt;hr&gt;

&lt;h5&gt;&lt;strong&gt;(b)&lt;/strong&gt; Find the volume of cylinder Q&lt;/h5&gt;

&lt;ol&gt;
  &lt;li&gt;Find the volume scale factor:&lt;br&gt;
  \( \text{Volume scale factor} = (\text{length scale factor})^3 = {@ scale_factor_length @}^3 = {@ scale_factor_volume @} \)&lt;/li&gt;&lt;br&gt;

  &lt;li&gt;Find the volume of Q:&lt;br&gt;
  \( V_Q = {@ V_P @} \times {@ scale_factor_volume @} = {@ V_Q @}\ \text{cm}^3 \)&lt;/li&gt;
&lt;/ol&gt;

&lt;p&gt;&lt;strong&gt;Answer (b):&lt;/strong&gt; {@ ta_ans2 @} cm³&lt;/p&gt;

&lt;hr&gt;

&lt;p&gt;&lt;em&gt;Summary:&lt;/em&gt;&lt;/p&gt;
&lt;ul&gt;
  &lt;li&gt;&lt;strong&gt;Length of cylinder Q:&lt;/strong&gt; {@ ta_ans1 @} cm&lt;/li&gt;
  &lt;li&gt;&lt;strong&gt;Volume of cylinder Q:&lt;/strong&gt; {@ ta_ans2 @} cm³&lt;/li&gt;
&lt;/ul&gt;</text>
    </generalfeedback>
