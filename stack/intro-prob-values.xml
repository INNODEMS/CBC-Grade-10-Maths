    <questionvariables original-id="intro-prob-values-1-1-9" assembly-id="intro-prob-values-1-1-9" unique-id="intro-prob-values-1-1-9">
      <text original-id="intro-prob-values-1-1-9-1" assembly-id="intro-prob-values-1-1-9-1" unique-id="intro-prob-values-1-1-9-1">/* Situation choice */
sit_choice: rand([1,2,3]);

/* Situation text */
situation_text:
  if sit_choice=1 then "picking a red ball from a bag that contains only red balls"
  elseif sit_choice=2 then "picking a green mango from a tree with ripe and unripe mangoes"
  else "picking a blue pen from a bag that has no blue pens";

/* Correct probability value */
correct_prob:
  if sit_choice=1 then "1"
  elseif sit_choice=2 then "Between 0 and 1"
  else "0";

/* Dropdown matrix */
m_prob: [
  ["0", is(correct_prob="0")],
  ["Between 0 and 1", is(correct_prob="Between 0 and 1")],
  ["1", is(correct_prob="1")]
];
</text>
    </questionvariables>
