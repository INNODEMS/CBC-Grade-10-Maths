    <questiontext format="moodle_auto_format" original-id="similar-triangles-unknown-sides-1-1-2" assembly-id="similar-triangles-unknown-sides-1-1-2" unique-id="similar-triangles-unknown-sides-1-1-2">
      <text original-id="similar-triangles-unknown-sides-1-1-2-1" assembly-id="similar-triangles-unknown-sides-1-1-2-1" unique-id="similar-triangles-unknown-sides-1-1-2-1">&lt;p&gt;Are triangle \(ABC\) in &lt;b&gt;Fig 1&lt;/b&gt; and triangle \(PQR\) in &lt;b&gt; Fig 2&lt;/b&gt; similar?&lt;/p&gt;
[[jsxgraph]]
var board = JXG.JSXGraph.initBoard('jxgbox', {
    boundingbox: [-2, 12, 16, -2],
    axis: false,
    pan: { enabled: false },
    zoom: { enabled: false },
    showNavigation: false
});

/* === Triangle LMN (smaller, top-left) === */
var L = board.create('point', [1,10], {name:'P', fixed:true, size:2});
var M = board.create('point', [0,8], {name:'Q', fixed:true, visible:true});
var N = board.create('point', [3,8], {name:'R', fixed:true, visible:true});

function edge(P1,P2){ return board.create('segment',[P1,P2],{strokeWidth:2,fixed:true}); }
edge(L,M); edge(M,N); edge(N,L);

// Side labels
board.create('text',[()=&gt;(L.X()+M.X())/6, ()=&gt;(L.Y()+M.Y())/2+0.2, "{#PQ#} cm"]);
board.create('text',[()=&gt;(L.X()+N.X())/2, ()=&gt;(L.Y()+N.Y())/2+0.2, "{#PR#} cm"]);
board.create('text',[()=&gt;(M.X()+N.X())/3, ()=&gt;(M.Y()+N.Y())/2-0.2, "{#letter2#} cm"]);

// Angles near apexes
board.create('text',[L.X()-0.1, L.Y()-0.3, "55째"]);
board.create('text',[M.X()+2.4, M.Y()+0.2, "50째"]);

/* === Triangle ABC (larger, bottom-right, closer) === */
var A = board.create('point', [6,7], {name:'A', fixed:true, size:2});
var B = board.create('point', [4.5,3], {name:'B', fixed:true, visible:true});
var C = board.create('point', [9,3], {name:'C', fixed:true, visible:true});

edge(A,B); edge(B,C); edge(C,A);

// Side labels
board.create('text',[()=&gt;(A.X()+B.X())/2.2, ()=&gt;(A.Y()+B.Y())/2+0.2, "{#AB#} cm"]);
board.create('text',[()=&gt;(A.X()+C.X())/2, ()=&gt;(A.Y()+C.Y())/2+0.2, "{#letter1#} cm"]);
board.create('text',[()=&gt;(B.X()+C.X())/2, ()=&gt;(B.Y()+C.Y())/2-0.2, "{#BC#} cm"]);

// Angles near apexes
board.create('text',[A.X()-0.1, A.Y()-0.4, "55째"]);
board.create('text',[C.X()-0.6, C.Y()+0.2, "50째"]);

// Label for first triangle
board.create('text', [
    () =&gt; L.X() - 0.0,
    () =&gt; L.Y() -2.6,
    "Fig 1"
], {
    fontSize: 16,
    fontWeight: 'bold'
});

// Label for second triangle
board.create('text', [
    () =&gt; A.X() + 0.2,
    () =&gt; A.Y() - 4.5,
    "Fig 2"
], {
    fontSize: 16,
    fontWeight: 'bold'
});


/* Prevent interaction */
board.renderer.svgRoot.style.pointerEvents = "none";

[[/jsxgraph]]
&lt;p&gt;[[input:ans1]] [[validation:ans1]][[feedback:prt1]]  &lt;/p&gt;
&lt;p&gt;Hence find the values of {@letter1@} = [[input:ans2]] cm[[validation:ans2]] and {@letter2@} = [[input:ans3]] cm[[validation:ans3]]&lt;/p&gt;.
&lt;p&gt;[[feedback:prt2]]&lt;/p&gt;</text>
