    <questionvariables original-id="classify-rational-list-1-1-9" assembly-id="classify-rational-list-1-1-9" unique-id="classify-rational-list-1-1-9">
      <text original-id="classify-rational-list-1-1-9-1" assembly-id="classify-rational-list-1-1-9-1" unique-id="classify-rational-list-1-1-9-1">/* Pool of rational expressions (true) */
rationals_all: [
    5, -4, 7/3, 0.25, 3/10, sqrt(25), sqrt(49),
    12/5, 0.12121212, -9, 4/7, 0, -13, 1.5,
    2/3, sqrt(36), sqrt(100), 0.33333333
];

/* Pool of irrational expressions (false) */
irrationals_all: [
    pi, sqrt(2), sqrt(3), sqrt(5), 2*sqrt(2),
    pi - 3, sqrt(8), 0.121221222, 3*sqrt(7),
    sqrt(10), e, sqrt(11), pi/2, sqrt(13)
];

/* Select correct answers (rational numbers) */
num_correct: 3;
correct_answers: rand_selection([
    [1, true, rationals_all[1]],
    [2, true, rationals_all[2]],
    [3, true, rationals_all[3]],
    [4, true, rationals_all[4]],
    [5, true, rationals_all[5]],
    [6, true, rationals_all[6]],
    [7, true, rationals_all[7]],
    [8, true, rationals_all[8]],
    [9, true, rationals_all[9]],
    [10, true, rationals_all[10]]
], num_correct);

/* Select incorrect answers (irrational numbers) */
num_incorrect: 3;
incorrect_answers: rand_selection([
    [11, false, irrationals_all[1]],
    [12, false, irrationals_all[2]],
    [13, false, irrationals_all[3]],
    [14, false, irrationals_all[4]],
    [15, false, irrationals_all[5]],
    [16, false, irrationals_all[6]],
    [17, false, irrationals_all[7]],
    [18, false, irrationals_all[8]],
    [19, false, irrationals_all[9]],
    [20, false, irrationals_all[10]]
], num_incorrect);

/* Combine and randomize */
ta: random_permutation(append(correct_answers, incorrect_answers));
ta_largest: length(ta);
</text>
