    <questionvariables original-id="enlargement-triangle-origin-1-1-9" assembly-id="enlargement-triangle-origin-1-1-9" unique-id="enlargement-triangle-origin-1-1-9">
      <text original-id="enlargement-triangle-origin-1-1-9-1" assembly-id="enlargement-triangle-origin-1-1-9-1" unique-id="enlargement-triangle-origin-1-1-9-1">Ax:rand_with_step(-9,9,1);
Ay:rand_with_prohib(1,9,[Ax]);

Bx:rand_with_prohib(1,9,[Ax,Ay]);
By:rand_with_prohib(1,9,[Ax,Ay,Bx]);

Cx:rand_with_prohib(1,9,[Ax,Ay,Bx,By]);
Cy:rand_with_prohib(1,9,[Ax,Ay,Bx,By,Ax]);
/*scale factor*/
k:rand([-2,-1,2,3,1/2,4]);
/*coodinates of the image*/
Ax1:k*Ax;
Ay1:k*Ay;

Bx1:k*Bx;
By1:k*By;

Cx1:k*Cx;
Cy1:k*Cy;
ta:[Ax1,Ay1];
ta1:[Bx1,By1];
ta2:[Cx1,Cy1];</text>
