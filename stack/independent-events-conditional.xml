    <questionvariables original-id="independent-events-conditional-1-1-9" assembly-id="independent-events-conditional-1-1-9" unique-id="independent-events-conditional-1-1-9">
      <text original-id="independent-events-conditional-1-1-9-1" assembly-id="independent-events-conditional-1-1-9-1" unique-id="independent-events-conditional-1-1-9-1">/*Defining question variables*/
aa:rand(70)+20;/*percentage time of 0s*/
bb:100-aa;/*percentage time of 1s*/
cc:rand(5)+1;/*percentage chance of obtaining any particular symbol distorted*/
dd:aa/100;
p_0:decimalplaces(float(dd),2);/*p('0' is transmitted)*/
ee:bb/100;
p_1:decimalplaces(float(ee),2);/*p('1' is transmitted)*/
ff:cc/100;
p_1_0:decimalplaces(float(ff),2);/*p(1 is received|0 is transmitted)*/
gg:(100-cc)/100;
p_1_1:decimalplaces(float(gg),2);/*p(1 is received|1 is transmitted)*/
p_r1:(p_1_0*p_0)+(p_1_1*p_1);/*p(receive 1)*/

/*Model answer*/
ta:decimalplaces(p_r1,2);</text>
    </questionvariables>
