    <questionvariables original-id="exponential-to-log-form-1-1-9" assembly-id="exponential-to-log-form-1-1-9" unique-id="exponential-to-log-form-1-1-9">
      <text original-id="exponential-to-log-form-1-1-9-1" assembly-id="exponential-to-log-form-1-1-9-1" unique-id="exponential-to-log-form-1-1-9-1">/* Random base for logarithm */
base: rand([2,3,5,10]);

/* Random exponent */
exp: rand([2,3,4,5]);

/* Resulting number */
number: base^exp;

/* Correct logarithmic expression in STACK form */
log_expr: log(number)/log(base);

/* Common mistakes */
m1: log(exp)/log(base);        /* wrong input */
m2: log(base)/log(number);     /* reversed */
m3: exp^base;                  /* nonsense algebraic error */
</text>
