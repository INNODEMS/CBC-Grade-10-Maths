    <questionvariables original-id="logs-standard-form-1-1-9" assembly-id="logs-standard-form-1-1-9" unique-id="logs-standard-form-1-1-9">
      <text original-id="logs-standard-form-1-1-9-1" assembly-id="logs-standard-form-1-1-9-1" unique-id="logs-standard-form-1-1-9-1">/* Choose a power of 10 */
exp: rand([3,4,5,6]);

/* Number formed using power of 10 */
number: 10^exp;

/* Coefficient between 1 and 9 */
coef: rand_with_step(1,9,1);

/* Final number in expanded form */
value: coef * number;

/* Standard form components */
A: coef;
B: exp;

/* Wrong answers */
m1: coef + exp;      /* adds instead of SF */
m2: coef * exp;      /* multiplies instead */
m3: number;          /* gives full number instead of SF */
</text>
    </questionvariables>
