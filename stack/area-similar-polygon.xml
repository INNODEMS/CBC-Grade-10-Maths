    <questionvariables original-id="area-similar-polygon-1-1-9" assembly-id="area-similar-polygon-1-1-9" unique-id="area-similar-polygon-1-1-9">
      <text original-id="area-similar-polygon-1-1-9-1" assembly-id="area-similar-polygon-1-1-9-1" unique-id="area-similar-polygon-1-1-9-1"> /* Side lengths of similar regular polygons */
a : rand(9) + 3;   /* side length of smaller polygon */
b : a + rand(9) + 1; /* side length of larger polygon, ensure b &gt; a */

/* Area of the smaller polygon */
c : rand_with_step(25,300,5) ;

/* Scale factor between corresponding sides */
k : b / a;

/* Area of the larger hexagon (area scales with square of side ratio) */
 larger_hexagon : c * k^2;
ta:decimalplaces( larger_hexagon,2);

/*mistakes*/
/*Using the linear scale factor instead of the area scale factor*/
ta1:decimalplaces(c*k,2);
/*Forgetting which area is given*/
ta2:decimalplaces((1/k^2)*c,2);</text>
    </questionvariables>
