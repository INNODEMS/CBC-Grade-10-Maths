    <questionvariables original-id="capacity-larger-container-1-1-9" assembly-id="capacity-larger-container-1-1-9" unique-id="capacity-larger-container-1-1-9">
      <text original-id="capacity-larger-container-1-1-9-1" assembly-id="capacity-larger-container-1-1-9-1" unique-id="capacity-larger-container-1-1-9-1"> /* Side lengths of similar regular polygons */
a : rand(9) + 3;   /* side length of smaller polygon */
b : a + rand(9) + 1; /* side length of larger polygon, ensure b &gt; a */

/* volume of the smaller container */
c : rand_with_step(25,300,5) ;

/* Scale factor between corresponding sides */
k : b / a;
  /*cubic scale*/
b1:b^3;
a1:a^3;

/* volume of the larger container (area scales with square of side ratio) */
 larger_container : c * k^3;
ta:decimalplaces( larger_container,2);


/*mistakes*/
ta1:k*c;
ta2:decimalplaces((1/k^3)*c,2);
ta3:c * k^2;</text>
    </questionvariables>
