    <questionvariables original-id="divider-arm-length-45-1-1-9" assembly-id="divider-arm-length-45-1-1-9" unique-id="divider-arm-length-45-1-1-9">
      <text original-id="divider-arm-length-45-1-1-9-1" assembly-id="divider-arm-length-45-1-1-9-1" unique-id="divider-arm-length-45-1-1-9-1">/* Randomize the vertical distance from 5 to 25 cm in steps of 1 */
d: rand_with_step(1,56,4);

/* Angle of 45 degrees */
theta: 45;

/* Convert degrees to radians for computation */
theta_rad: theta * %pi/180;

/* Length of one arm of the divider */
arm_length: d / sin(theta_rad);</text>
