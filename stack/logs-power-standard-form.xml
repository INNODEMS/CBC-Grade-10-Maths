    <questionvariables original-id="logs-power-standard-form-1-1-9" assembly-id="logs-power-standard-form-1-1-9" unique-id="logs-power-standard-form-1-1-9">
      <text original-id="logs-power-standard-form-1-1-9-1" assembly-id="logs-power-standard-form-1-1-9-1" unique-id="logs-power-standard-form-1-1-9-1">/* Large number between 10^4 and 10^8 */
exp2: rand([4,5,6,7,8]);
coef2: rand_with_step(1.2,9.8,0.2);

/* Construct value */
value2: coef2 * 10^exp2;

/* Standard form targets */
A2: coef2;
B2: exp2;

/* Log components */
log_value: log(value2)/log(10);

/* Wrong answers */
m1: log(coef2)/log(10);        /* ignores power */
m2: exp2;                      /* forgets coefficient */
m3: coef2 + exp2;              /* confusion */
</text>
    </questionvariables>
