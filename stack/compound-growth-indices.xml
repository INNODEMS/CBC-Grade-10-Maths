    <questionvariables original-id="compound-growth-indices-1-1-9" assembly-id="compound-growth-indices-1-1-9" unique-id="compound-growth-indices-1-1-9">
      <text original-id="compound-growth-indices-1-1-9-1" assembly-id="compound-growth-indices-1-1-9-1" unique-id="compound-growth-indices-1-1-9-1">/* Principal amount in shillings */
P: rand_with_step(500,1500,100);

/* Growth multiplier (e.g. 1.05 means 5% growth) */
rate_choices: [105/100, 110/100, 120/100];
r: rand(rate_choices);

/* Number of years */
n: rand([2,3,4]);

/* Final amount */
A: P * r^n;

/* Wrong answers */
m1: P * r * n;        /* treats as simple growth */
m2: P * r^(n+1);      /* adds an extra growth cycle */
m3: P + r^n;          /* adds instead of multiplies */
</text>
    </questionvariables>
