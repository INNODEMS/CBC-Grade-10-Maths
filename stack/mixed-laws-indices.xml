    <questionvariables original-id="mixed-laws-indices-1-1-9" assembly-id="mixed-laws-indices-1-1-9" unique-id="mixed-laws-indices-1-1-9">
      <text original-id="mixed-laws-indices-1-1-9-1" assembly-id="mixed-laws-indices-1-1-9-1" unique-id="mixed-laws-indices-1-1-9-1">/* Random base */
base: rand([2,3,5,7]);

/* Random exponents for a mixed-law expression */
a: rand([2,3,4,5]);
b: rand([1,2,3]);
c: rand([1,2,3]);

/* Expression: (base^a * base^b) / base^c */
exp_total: (a + b) - c;   /* correct simplified exponent */

/* Full expression (for teacher notes) */
expr: (base^a * base^b) / base^c;

/* Common mistakes */
m1: base^(a + b + c);        /* adds all exponents */
m2: base^(a - b - c);        /* subtracts incorrectly */
m3: base^(a*b - c);          /* multiplies a and b */
</text>
    </questionvariables>
