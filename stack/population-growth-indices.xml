    <questionvariables original-id="population-growth-indices-1-1-9" assembly-id="population-growth-indices-1-1-9" unique-id="population-growth-indices-1-1-9">
      <text original-id="population-growth-indices-1-1-9-1" assembly-id="population-growth-indices-1-1-9-1" unique-id="population-growth-indices-1-1-9-1">/* Initial population */
P0: rand_with_step(800,1500,50);

/* Annual growth multiplier */
g_choices: [103/100, 104/100, 106/100];
g: rand(g_choices);

/* Number of years */
t: rand([3,4,5,6]);

/* Final population */
Pt: P0 * g^t;

/* Wrong answers */
m1: P0 * g * t;           /* linear growth mistake */
m2: P0 + g^t;             /* add instead of multiply */
m3: P0 * g^(t-1);         /* misses one cycle */
</text>
    </questionvariables>
