        <exercise xml:id="auto_5w12kmny">
            <statement>
                <p xml:id="auto_ufp1gfeu">
                    A factory records the number of products manufactured in a week:
                </p>
                <p xml:id="auto_q4dp2ee8">
                    <m>150, 130, 220, 135, 180, 140, 125, 250, 145, 230, 200, 205, 145, 190, 155,</m>
                </p>
                <p xml:id="auto_cs5cz55n">
                    <m>210, 225, 240, 135, 165, 245, 170, 175, 185, 130, 190, 195, 160, 170, 150,</m>
                </p>
                <p xml:id="auto_n1y81mkz">
                    <m>160, 120, 200, 210, 220, 235, 180, 230, 240, 215</m>
                </p>
                <p xml:id="auto_5ek3gyw0">
                    <ol xml:id="auto_5mx9npaf" marker="a)">
                        <li xml:id="auto_t59yiba5">
                            <p xml:id="auto_yya9ddpl">
                                Make a frequency distribution table for the set of data.
                            </p>
                        </li>
                        <li xml:id="auto_w8spq4wq">
                            <p xml:id="auto_i5vgbrpg">
                                Calculate the mean, mode and median.
                            </p>
                        </li>
                    </ol>
                </p>
            </statement>
            <answer>
                <p>
                    <ol marker="a)">
                        <li>
                            <p>
                                
                            </p>
                            <table xml:id="table_products_manufactured">
                                <title>Frequency Distribution of Products Manufactured</title>
                                <tabular left="medium" right="medium" top="medium" bottom="medium">
                                    <row halign="left" header="yes">
                                        <cell>Number of Products</cell>
                                        <cell>Frequency (f)</cell>
                                    </row>
                                    <row>
                                        <cell><m>120 - 139</m></cell>
                                        <cell><m>7</m></cell>
                                    </row>
