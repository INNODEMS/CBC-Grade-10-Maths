                <p xml:id="auto_0biifq98">
                    <ol xml:id="auto_wf6llwb4">
                        <li xml:id="auto_e4j36g5u">
                            <p xml:id="auto_alj2om8j">
                                Plot a displacement-time graph using the data above
                            </p>
                        </li>
                        <li xml:id="auto_6bl4ik0s">
