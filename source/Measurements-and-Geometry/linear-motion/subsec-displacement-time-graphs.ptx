            <image width="60%">
                <latex-image label="Displacement Time Graph for Uniform Acceleration">
                    \begin{tikzpicture}
                         % Graph Paper
                        \draw[gray, thin, step=0.5] (0,0) grid (7,7);

                        % Axes
                        \draw[thick,->] (0,0) -- (6.5,0) node[below] {\text{Time (s)}};
                        \draw[thick,->] (0,0) -- (0,7.0) node[left] {\text{Displacement (m)}};

                        % Y-axis labels outside
                        \foreach \y in {2,4,6,8,10,12,13}
                            \draw (-0.2,\y/2) -- (0.2,\y/2) node[left=6pt] {\y};

                        % X-axis labels
                        \foreach \x in {1,2,3,4,5}
                            \draw (\x,-0.2) -- (\x,0.2) node[below=6pt] {\x};

                        % Graph Title
                        \node[above] at (3,7.5) {\text{Displacement-Time Graph for Uniform Acceleration}};

                        % Parabolic Curve
                        \draw[thick,red] plot[smooth] coordinates {(0,0) (1,0.25) (2,1) (3,2.25) (4,4) (5,6.25)};

                        % Mark Displacement at t=5s
