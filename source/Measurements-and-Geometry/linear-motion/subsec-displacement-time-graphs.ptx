                            % Axes
                            \draw[thick,->] (0,0) -- (8.5,0) node[below] {\text{Time (s)}};
                            \draw[thick,->] (0,0) -- (0,5.5) node[left] {\text{Displacement (m)}};

                            % Y-axis labels outside
                            \foreach \y in {0,5,10,15,20}
