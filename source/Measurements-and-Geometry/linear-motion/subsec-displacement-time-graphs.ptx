                            % Graph Title
                            \node[above] at (4.5,5.5) { \text{Displacement-Time Graph for Car Motion}};

                            % Graph Line
                            \draw[thick,blue] (0,0) -- (1,0.8) -- (2,2) -- (3,3.2) -- (4,4) -- (5,4) -- (6,3) -- (7,1.6) -- (8,0);
