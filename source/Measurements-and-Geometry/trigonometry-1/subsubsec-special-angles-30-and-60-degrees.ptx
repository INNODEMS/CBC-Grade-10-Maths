            <exercise xml:id="auto_tul8akt4">
            <statement>
                <p xml:id="auto_220jm0l3">
                    <ol>
                        <li xml:id="auto_4w68s78o">
                            <p xml:id="auto_lev0jgaw">
                                Given that <m>\theta=45^\circ</m>, calculate:
                            </p>
                            <p xml:id="auto_88imvhqv">
                                <m>tan^2\,\theta-sin^2\,\theta</m>
