    <p xml:id="auto_afyk420x">
      <term>Materials</term>
    </p>
    <p xml:id="auto_6y84x4k9">
      <ul xml:id="auto_g2k0bgps">
        <li xml:id="auto_f73lvw6o">
          <p xml:id="auto_f3i01x95">
            A printed copy of the figure.
          </p>
        </li>
