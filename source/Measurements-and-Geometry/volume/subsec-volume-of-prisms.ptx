                        % Draw the bridge deck
                        \filldraw[fill=bridgegray, draw=black] (-4,2) rectangle (4,3);

                        % Draw the triangular prism supports (left)
                        \filldraw[fill=supportbrown, draw=black, opacity=0.7] 
                            (-3,0) -- (-1,2) -- (-5,2) -- cycle; % Front face
                        \filldraw[fill=supportbrown, draw=black, opacity=0.5] 
                            (-3,0) -- (-1,2) -- (-3,3) -- cycle; % Side face
                        \filldraw[fill=supportbrown, draw=black, opacity=0.5] 
                            (-3,0) -- (-5,2) -- (-3,3) -- cycle; % Side face

                        % Draw the triangular prism supports (right)
                        \filldraw[fill=supportbrown, draw=black, opacity=0.7] 
                            (3,0) -- (5,2) -- (1,2) -- cycle; % Front face
                        \filldraw[fill=supportbrown, draw=black, opacity=0.5] 
                            (3,0) -- (5,2) -- (3,3) -- cycle; % Side face
                        \filldraw[fill=supportbrown, draw=black, opacity=0.5] 
                            (3,0) -- (1,2) -- (3,3) -- cycle; % Side face

