    <subsubsection xml:id="subsubsec-finding-reciprocals-using-a-calculator">
        <title>Finding reciprocals using a calculator</title>
        
               
        <activity xml:id="auto_x75j1z1u">
            <statement>
                <p xml:id="auto_cto7tonr">
                    
                </p>
                <p xml:id="auto_yug44nx1">
                   1. Working in groups, use a calculator to work out the reciprocal of <m>151.6</m>
                </p>
                <p xml:id="auto_ncbd82jd">
                    <ol xml:id="auto_l265hvs3" marker="i)">
                        <li xml:id="auto_rziuwcq5">
                            <p xml:id="auto_jtqdxjme">
