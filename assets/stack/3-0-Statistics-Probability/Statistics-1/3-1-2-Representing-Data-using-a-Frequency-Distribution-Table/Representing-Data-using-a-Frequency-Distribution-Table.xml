    <questionvariables>
      <text><![CDATA[/* 1. Generate 7 UNIQUE values for the wait times (e.g., from range 2 to 25) */
/* We create a list of candidates, shuffle it, and take the first 7 */
candidates: makelist(i, i, 2, 25);
shuffled_cands: random_permutation(candidates);
/* Sort them so the table looks organized (ascending order) */
vals: sort(makelist(shuffled_cands[i], i, 1, 7));

/* Extract individual values for the table display */
v1: vals[1];
v2: vals[2];
v3: vals[3];
v4: vals[4];
v5: vals[5];
v6: vals[6];
v7: vals[7];

/* 2. Generate 7 Random Frequencies that sum exactly to 30 */
/* Start with 1 for each to ensure every number appears at least once */
freqs: [1, 1, 1, 1, 1, 1, 1];
/* Distribute the remaining 23 counts randomly among the 7 bins */
for i:1 thru 23 do (
   idx: rand(7) + 1,
   freqs[idx]: freqs[idx] + 1
);

/* Extract correct frequencies for grading */
f1: freqs[1];
f2: freqs[2];
f3: freqs[3];
f4: freqs[4];
f5: freqs[5];
f6: freqs[6];
f7: freqs[7];

/* 3. Build the Raw Data List */
raw_list: [];
/* Loop through our 7 values and add them to the list f_i times */
for i:1 thru 7 do (
   val: vals[i],
   count: freqs[i],
   for k:1 thru count do raw_list: endcons(val, raw_list)
);

/* 4. Shuffle the final list of 30 items for display */
final_data: random_permutation(raw_list);

/* Create the string for display */
data_str: sconcat(final_data[1]);
for i:2 thru 30 do data_str: sconcat(data_str, ", ", final_data[i]);]]></text>
    </questionvariables>
