    <questionvariables>
      <text>/* Random angle between 5 and 90 with step 0.003 */
angle : 5 + 0.02 * rand(floor((90-5)/0.02) + 1);

/* Sine value */
sine_value : sin(angle * %pi/180);
sine_value_decimal : float(sine_value);
ta:decimalplaces(sine_value_decimal ,4);

/* Cosine value */
cosine_value : cos(angle * %pi/180);
cosine_value_decimal : float(cosine_value);
ta_2:ta_1:decimalplaces(cosine_value_decimal,4);

/* Tangent value */
tangent_value : tan(angle * %pi/180);
tangent_value_decimal : float(tangent_value);
ta_3:ta_1:decimalplaces(tangent_value_decimal,4);</text>
    </questionvariables>
