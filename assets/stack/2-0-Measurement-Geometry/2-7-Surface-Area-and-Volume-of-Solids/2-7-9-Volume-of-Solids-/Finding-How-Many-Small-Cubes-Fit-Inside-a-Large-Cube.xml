small_side : (rand(20) + 1) * 3;    /* Small cube side: multiples of 10 cm */

/* Calculate volumes */
V_large : large_side^3;
V_small : small_side^3;

/* Number of small cubes (whole number division) */
num_cubes : V_large / V_small;
Number:round(num_cubes);</text>
