    <questionvariables>
      <text><![CDATA[/* 1. SETUP CLASSES AND RAW DATA */
/* Define class intervals: 10-19, 20-29, ... 70-79 */
lower_bounds: [10, 20, 30, 40, 50, 60, 70];
upper_bounds: [19, 29, 39, 49, 59, 69, 79];
class_width: 10;
num_classes: length(lower_bounds);

/* Generate 40 random test scores between 10 and 79 */
/* We sort them only for the 'worked solution' display variable, but keep raw for the question */
n_students: 40;
raw_data: makelist(rand(70)+10, k, 1, n_students);
raw_data_sorted: sort(raw_data);

/* 2. BIN THE DATA (Count Frequencies) */
/* Function to count how many numbers fall in a range [a, b] */
count_in_range(list, a, b) := length(sublist(list, lambda([x], x >= a and x <= b)));

/* Create the list of correct frequencies */
freqs: makelist(count_in_range(raw_data, lower_bounds[i], upper_bounds[i]), i, 1, num_classes);

/* 3. GROUPED STATISTICS CALCULATIONS */
/* Midpoints */
midpoints: map(lambda([l, u], (l + u)/2), lower_bounds, upper_bounds);

/* Mean Calculation (Grouped) */
fx_list: map("*", freqs, midpoints);
sum_fx: apply("+", fx_list);
sum_f: apply("+", freqs); /* Should equal n_students */
mean_est: float(sum_fx / sum_f);

/* Modal Class */
max_freq: lmax(freqs);
/* Find index of max freq */
mode_idx: sublist_indices(freqs, lambda([x], x=max_freq))[1];
/* Format string for dropdown answer */
mode_str: sconcat(lower_bounds[mode_idx], " - ", upper_bounds[mode_idx]);

/* Median Calculation (Grouped Interpolation) */
/* Cumulative Frequencies */
cum_freqs: [];
running: 0;
for f in freqs do (running: running + f, cum_freqs: endcons(running, cum_freqs));

/* Find Median Class */
med_pos: sum_f / 2;
med_idx: 1;
for i:1 thru num_classes do (
    if cum_freqs[i] >= med_pos then (med_idx: i, return())
);

/* Median Variables */
L_med: lower_bounds[med_idx]; /* Using lower bound of class (e.g. 30). Note: strictly speaking boundaries are x.5, but standard school math often uses integer lower bound. Adjust to L_med - 0.5 if your curriculum requires continuous boundaries. */
f_med: freqs[med_idx];
cf_prev: if med_idx = 1 then 0 else cum_freqs[med_idx - 1];
h: class_width;

median_est: float(L_med + ((med_pos - cf_prev) / f_med) * h);

/* 4. DISPLAY HELPERS */
/* Create a string of the raw data for display */
data_str: sconcat(raw_data);]]></text>
