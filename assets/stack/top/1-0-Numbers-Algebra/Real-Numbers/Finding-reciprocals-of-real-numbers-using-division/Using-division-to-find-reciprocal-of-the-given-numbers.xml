    <questionvariables>
      <text><![CDATA[/* Positive integer: 1 to 20 */
a: rand(20) + 1;

/* Negative integer: -1 to -20 */
b: -(rand(20) + 1);

/* Decimal between 0.1 and 5 with 1 decimal place */
c: (rand(50) + 1)/10;

/* Negative decimal between -0.1 and -5 with 1 decimal place */
d: -(rand(50) + 1)/10;

/* Positive fraction a/b where a < b */
num1: rand(8) + 2;
den1: num1 + rand(8) + 1;
e: num1/den1;

/* Negative fraction a/b where a < b */
num2: rand(8) + 2;
den2: num2 + rand(8) + 1;
f: -num2/den2;

/* Calculate reciprocals using division: 1 รท number */
ta1: 1/a;
ta2: 1/b;
ta3: 1/c;
ta4: 1/d;
ta5: 1/e;
ta6: 1/f;
/*mistakes*/
ta1_mistake:-a;
ta2_mistake:-1*(b);
ta3_mistake:-c;
ta4_mistake:-1*(d);
ta5_mistake:-e;
ta6_mistake:-1*(f);
]]></text>
