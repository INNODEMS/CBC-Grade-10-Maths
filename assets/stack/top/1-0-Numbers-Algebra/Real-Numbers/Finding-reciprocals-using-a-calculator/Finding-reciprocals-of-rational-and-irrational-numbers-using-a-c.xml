    <questionvariables>
      <text>number1:rand(1000)+4;
a:sqrt(number1);
recip_a:float(1.0/a);
ta1:decimalplaces(recip_a,4);
number2:rand(10)+1;
b :number2/ %pi;
recip_b:float(1.0/b);
ta2:decimalplaces(recip_b,4);
num:rand(10)+1;
den:rand_with_prohib(5,35,[num]);
frac1:num/den;
ta3:1/frac1;
mistake_ta3:-1/frac1;
/*mistake*/
mistake1:number2/ 3.142;
recip_b1:float(1.0/mistake1);
ta2_mistake:decimalplaces(recip_b1,4);
</text>
    </questionvariables>
